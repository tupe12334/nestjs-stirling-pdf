{
  "name": "nestjs-stirling-pdf",
  "version": "0.0.1",
  "description": "A NestJS module for seamless integration with Stirling PDF instances",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepare": "pnpm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "generate:sdk": "pnpm dlx @openapitools/openapi-generator-cli generate -i https://api.swaggerhub.com/apis/Frooodle/Stirling-PDF/0.45.0 -g typescript-axios -o ./src/generated"
  },
  "keywords": [
    "nestjs",
    "stirling-pdf",
    "pdf",
    "openapi",
    "typescript",
    "axios"
  ],
  "author": "tupe12334",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tupe12334/nestjs-stirling-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/tupe12334/nestjs-stirling-pdf/issues"
  },
  "homepage": "https://github.com/tupe12334/nestjs-stirling-pdf#readme",
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-agent": "^1.0.17",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/generated/**",
      "!src/**/*.spec.ts",
      "!src/**/*.test.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
